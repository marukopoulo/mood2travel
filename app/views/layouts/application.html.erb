<head>
  <title>Mood2travel</title>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link rel="shortcut icon" href="/images/icons/favicon.ico" >
  <%= csrf_meta_tags %>
  <script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
  <script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>

</head>



<body>
  <nav class="navbar navbar-inverse">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/"><img style = "max-width:280px;margin-top:-6px;"src= "/images/icons/logo.png"  alt = "mood2travel"></a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          
          <ul class="nav navbar-nav navbar-right">
            <li><a href="/recomand">Seach From Mood</a></li>
             <% if user_signed_in? %>
             <li><%= link_to "My page" , user_path(:id => current_user)  %></li>
              <li><%= link_to "Sign out", destroy_user_session_path, :method => :delete %></li>
             <% else %>
            <li><a href="/users/sign_up">Sign up</a></li>
            <li ><a href="/users/sign_in">Sign in </a></li>
            <% end %>
            <li ><a href="/items/new">Uploads</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>
